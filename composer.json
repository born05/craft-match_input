{
  "name": "born05/craft-matchinput",
  "description": "Craft field type for text fields that match a regex pattern",
  "version": "3.0.0",
  "type": "craft-plugin",
  "keywords": [
    "craft", "craftcms", "craft-plugin", "match input"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marion Newlevant",
      "homepage": "http://marion.newlevant.com"
    },
    {
      "name": "Born05",
      "homepage": "https://www.born05.com/"
    }
  ],
  "require": {
    "craftcms/cms": "^4.0.0|^5.0.0-beta.1"
  },
  "autoload": {
    "psr-4": {
      "born05\\matchinput\\": "src/"
    }
  },
  "extra": {
    "name": "Match Input",
    "handle": "matchinput",
    "hasSettings": false,
    "hasCpSection": false,
    "documentationUrl": "https://github.com/born05/craft-matchinput/blob/main/README.md",
    "changelogUrl": "https://raw.githubusercontent.com/born05/craft-matchinput/main/CHANGELOG.md"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
